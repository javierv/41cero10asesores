- title 'Páginas borradas'

= table_for @versiones do |table|
  = table.data do
    = table.cell :versioned, heading: 'Título' do |version|
      - version.restore.titulo
    = table.cell :created_at, heading: 'Borrada el' do |version|
      - l version.created_at, format: :long
    = actions_cell(table) { |version| version.borradas_actions_list }
